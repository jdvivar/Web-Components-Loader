{
    "name": "Web Components Loader",
    "description": "Extension to load Web Components definitions",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "hello.html",
        "default_icon": "hello_extensions.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*/*"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ]
}